    "husky": "^0.11.3",
    "jest": "^24.0.0",
    "jscodeshift": "^0.11.0",
    "polished": "^4.0.3",
    "postcss": "^8.2.2",
    "postcss-loader": "^4.1.0",
    "prettier": "^1.18.2",
