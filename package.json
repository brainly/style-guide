  "scripts": {
    "clean": "rimraf dist",
    "js-lint": "eslint --ext .js --ext .jsx src/",
    "scss-lint": "stylelint 'src/**/*.scss'",
    "scss-unused-variables": "./scripts/find_scss_unused_variables.sh src/",
    "js-typecheck": "flow && node ./scripts/build-types.js",
    "build": "yarn clean && gulp build && build-storybook -o dist/latest/docs",
    "postversion": "git push && git push --tags",
    "test": "yarn run scss-lint && yarn run scss-unused-variables && yarn run js-typecheck && yarn run js-lint && yarn prettier-check && jest",
    "eslint": "./node_modules/.bin/eslint",
    "package-clean": "rimraf esm/ commonjs/ css/ types/ assets/ deprecated.yaml",
    "package-esm": "BABEL_ENV=esm babel src --out-dir esm --only 'src/logos.js' --only 'src/components' --only 'src/js' --only 'src/index.js' --only 'src/logo-url.js' --ignore '**/*.spec.jsx' --ignore '**/pages/**' --ignore '**/iframe-pages/**' --ignore '**/__mocks__/**'",
    "package-commonjs": "BABEL_ENV=commonjs babel src --out-dir commonjs --only 'src/logos.js' --only 'src/components' --only 'src/js' --only 'src/index.js' --only 'src/logo-url.js' --ignore '**/*.spec.jsx' --ignore '**/pages/**' --ignore '**/iframe-pages/**' --ignore '**/__mocks__/**'",
    "package-css": "node ./scripts/build-css.js",
    "package-deprecated": "node ./scripts/build-deprecated-list.js",
    "package-assets": "gulp build-package-assets",
    "package-types": "node ./scripts/build-types.js",
    "package-prepare": "yarn package-clean && concurrently --names 'commonjs,esm,css' 'yarn run package-commonjs' 'yarn run package-esm' 'yarn run package-css' 'yarn run package-assets' 'yarn run package-types' 'yarn run package-deprecated'",
    "prepublishOnly": "yarn package-prepare",
    "storybook": "start-storybook -p 6006",
    "storybook-chromatic": "STORYBOOK_ENV=chromatic start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "NODE_ENV=production STORYBOOK_ENV=chromatic npx chromatic --exit-zero-on-changes",
    "prettier-check": "prettier --check src",
    "prettier-fix": "prettier --write src",
    "run-codemod": "node ./bin/codemods/cli.js"
  },
  "jest": {
    "setupFiles": [
      "./scripts/testsSetup.js"
    ],
    "setupFilesAfterEnv": [
      "jest-axe/extend-expect"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/components",
      "src/docs"
    ],
    "transform": {
      ".jsx?$": "babel-jest"
    },
    "roots": [
      "<rootDir>/src"
    ]
  },
  "sideEffects": [
    "*.scss",
    "./src/images/*icons.js"
  ],
  "bin": {
    "sg-codemod": "./bin/codemods/cli.js"
  }
}
