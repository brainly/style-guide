{
  "name": "brainly-style-guide",
  "version": "179.0.0",
  "description": "Brainly Front-End Style Guide",
  "repository": "https://github.com/brainly/style-guide.git",
  "author": "Brainly Team",
  "license": "GPL-3.0-only",
  "module": "esm/index.js",
  "main": "commonjs/index.js",
  "files": [
    "src/",
    "esm/",
    "commonjs/"
  ],
  "engines": {
    "node": ">=8.0.0",
    "yarn": "^1.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "del": "^2.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^6.8.0",
    "eslint-config-brainly-react": "^2.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.3.0",
    "flow-bin": "^0.112.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-base64": "^0.1.3",
    "gulp-fingerprint": "^1.0.0",
    "gulp-fontcustom": "^0.1.2",
    "gulp-livereload": "^4.0.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-util": "^3.0.6",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "husky": "^0.11.3",
    "jest": "^26.4.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^1.2.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "s3": "^4.4.0",
    "sass-lint": "^1.10.2",
    "sass-lint-config-brainly": "^1.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.3.0",
    "terser-webpack-plugin": "^1.4.5",
    "through2": "^2.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "yargs": "^15.4.1"
  },
  "scripts": {
    "js-lint": "yarn run eslint --ext .js --ext .jsx src/",
    "scss-lint": "sass-lint -c node_modules/sass-lint-config-brainly/.sass-lint.yml -v -q 'src/**/*.scss'",
    "scss-unused-variables": "./scripts/find_scss_unused_variables.sh src/",
    "js-typecheck": "flow",
    "build": "gulp build",
    "start": "http-server ./dist -p 8000",
    "deploy": "gulp deploy",
    "postversion": "git push && git push --tags",
    "watch": "gulp build && gulp watch & yarn start",
    "test": "yarn run scss-lint && yarn run scss-unused-variables && yarn flow && yarn run js-lint && jest",
    "eslint": "./node_modules/.bin/eslint",
    "beta:watch": "webpack-dev-server --hot --inline --open --watch --mode=development",
    "package-clean": "yarn rimraf esm/ commonjs/",
    "package-esm": "BABEL_ENV=esm babel src --out-dir esm --only 'src/components' --only 'src/js' --only 'src/index.js' --ignore '**/*.spec.jsx' --ignore '**/pages/**' --ignore '**/iframe-pages/**' --ignore '**/__mocks__/**'",
    "package-commonjs": "BABEL_ENV=commonjs babel src --out-dir commonjs --only 'src/components' --only 'src/js' --only 'src/index.js' --ignore '**/*.spec.jsx' --ignore '**/pages/**' --ignore '**/iframe-pages/**' --ignore '**/__mocks__/**'",
    "package-prepare": "yarn package-clean && concurrently --names 'commonjs,esm' 'yarn run package-commonjs' 'yarn run package-esm'",
    "prepublishOnly": "yarn package-prepare"
  },
  "jest": {
    "setupFiles": [
      "./scripts/testsSetup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/components",
      "src/docs"
    ],
    "transform": {
      ".*": "babel-jest"
    }
  },
  "sideEffects": false
}
