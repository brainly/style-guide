{
  "name": "style-guide",
  "version": "83.12.0",
  "description": "Brainly Front-End Style Guide",
  "author": "Brainly",
  "private": true,
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "del": "^2.2.0",
    "enzyme": "^2.8.2",
    "eslint": "^4.0.0",
    "eslint-plugin-react": "^7.1.0",
    "frontend-tools-configs": "git+https://github.com/brainly/frontend-tools-configs.git#v9.0.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-base64": "^0.1.3",
    "gulp-fingerprint": "^0.3.2",
    "gulp-fontcustom": "0.1.1",
    "gulp-livereload": "^3.8.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-rev": "^7.0.0",
    "gulp-sass": "^2.0.1",
    "gulp-sourcemaps": "^1.9.1",
    "gulp-svg-sprite": "^1.2.7",
    "gulp-util": "^3.0.6",
    "http-server": "^0.9.0",
    "husky": "^0.11.3",
    "jest": "^20.0.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "run-sequence": "^1.1.1",
    "s3": "^4.4.0",
    "sass-lint": "^1.10.2",
    "through2": "^2.0.3",
    "webpack": "^2.6.1",
    "yargs": "^4.3.1"
  },
  "scripts": {
    "js-lint": "eslint --ext .js --ext .jsx src/",
    "scss-lint": "sass-lint -c node_modules/frontend-tools-configs/.sass-lint.yml -v -q 'src/**/*.scss'",
    "scss-unused-variables": "./scripts/find_scss_unused_variables.sh src/",
    "build": "gulp build",
    "start": "http-server ./dist -p 8000",
    "prepush": "npm test",
    "deploy": "gulp deploy",
    "postversion": "git push && git push --tags",
    "watch": "gulp build && gulp watch & npm start",
    "test": "npm run scss-lint -s && npm run scss-unused-variables -s  && npm run js-lint -s  && jest"
  }
}
