  "scripts": {
    "js-lint": "yarn eslint --ext .js --ext .jsx src/",
    "js-jest-eslint": "yarn jest --config jest-eslint.config.js",
    "scss-lint": "sass-lint --config `node -p 'require.resolve(\"sass-lint-config-brainly\")'` -v -q 'src/**/*.scss'",
    "scss-unused-variables": "./scripts/find_scss_unused_variables.sh src/",
    "js-typecheck": "flow",
    "build": "gulp build",
    "start": "http-server ./dist -p 8000",
    "deploy": "gulp deploy",
    "postversion": "git push && git push --tags",
    "watch": "gulp build && gulp watch & yarn start",
    "test": "yarn run scss-lint && yarn run scss-unused-variables && yarn flow && yarn run js-jest-eslint && jest"
  },
  "jest": {
    "setupFiles": [
      "./scripts/testsSetup.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/components",
      "src/docs"
    ],
    "transform": {
      ".*": "babel-jest"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5"
  }
}
