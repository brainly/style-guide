[ignore]
<PROJECT_ROOT>/node_modules/.*gulp-prettify
<PROJECT_ROOT>/node_modules/@khanacademy/flow-to-ts/test.json
<PROJECT_ROOT>/.typescript/**

[libs]

[lints]

[options]
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='scss' -> 'empty/object'

module.system.node.allow_root_relative=true
module.system.node.root_relative_dirname=node_modules
module.system.node.root_relative_dirname=src/components
module.system.node.root_relative_dirname=src/docs
exact_by_default=true

esproposal.optional_chaining=enable

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
