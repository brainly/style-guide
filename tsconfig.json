{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": false
  },
  "include": [".typescript/**/*"]
}
